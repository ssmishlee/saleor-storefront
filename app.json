{
  "name": "mishlee-storefront",
  "description": "A GraphQL-powered, PWA, single-page storefront application for mishlee",
  "repository": "https://github.com/mirumee/saleor-storefront",
  "website": "https://mishlee.com/",
  "logo": "https://mirumee.com/img/opensource_saleor.jpg",
  "keywords": ["mishlee", "e-commerce", "storefront"],
  "env": {
    "API_URI": {
      "description": "URI of a running instance of mishlee GraphQL API",
      "value": "https://api.mishlee.com/graphql/"
    }
  },
  "buildpacks": [
    {
      "url": "https://github.com/heroku/heroku-buildpack-nodejs.git"
    },
    {
      "url": "https://github.com/heroku/heroku-buildpack-static.git"
    }
  ]
}
